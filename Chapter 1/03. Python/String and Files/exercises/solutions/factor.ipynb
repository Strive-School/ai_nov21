    "# Factorial\n",
    "def factorial(number):\n",
    "    k = 1\n",
    "    while number > 0:\n",
    "        k *= number\n",
    "        number -= 1\n",
    "    return k                         \n",
    "\n",
    "    #Your Code Here\n",
    "\n",
    "print(factorial(3))"
   ]
  }
